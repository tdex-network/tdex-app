{
  "name": "tdex-app",
  "version": "1.2.6",
  "private": true,
  "description": "Mobile app for making trades on TDEX",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@ionic/eslint-config/recommended",
      "plugin:cypress/recommended",
      "react-app"
    ],
    "env": {
      "cypress/globals": true
    },
    "globals": {
      "module": true
    },
    "rules": {
      "@typescript-eslint/no-namespace": "off",
      "@typescript-eslint/ban-ts-comment": "off"
    }
  },
  "scripts": {
    "android": "ionic capacitor run android",
    "android:serve": "ionic capacitor run android -l --external",
    "build": "ionic build",
    "build:android": "ionic capacitor build android --no-open",
    "build:ios": "ionic capacitor build ios --no-open",
    "bump": "fastlane bump",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "eslint": "eslint \"./src/**/*.{ts,tsx}\" \"./test/**/*.{ts,tsx}\"",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write",
    "ios": "ionic capacitor run ios",
    "ios:serve": "ionic capacitor run ios -l --external",
    "lint": "npm run eslint && npm run prettier -- --check",
    "prettier": "prettier \"./src/**/*.{ts,tsx}\" \"./test/**/*.{ts,tsx}\"",
    "resources": "cordova-res ios --skip-config --copy && cordova-res android --skip-config --copy",
    "serve": "ionic serve",
    "serve:testnet": "REACT_APP_CHAIN=testnet ionic serve",
    "serve:no-open": "ionic serve --no-open",
    "sync": "ionic cap sync",
    "tdex:clean": "bash scripts/tdexd-clean.sh",
    "tdex:connect": "docker exec -it tdexd tdexdconnect --out url",
    "tdex:run": "bash scripts/tdexd-run",
    "tdex:run-init": "bash scripts/tdexd-run-initialized.sh",
    "tdex:run-initialized": "curl https://raw.githubusercontent.com/TDex-network/tdex-sdk/master/scripts/tdexd-run -o tdex-run && bash tdex-run && rm tdex-run",
    "test": "BABEL_ENV=test npx jest"
  },
  "dependencies": {
    "@capacitor-community/barcode-scanner": "^2.0.1",
    "@capacitor-community/react-hooks": "^0.0.11",
    "@capacitor/android": "^3.4.1",
    "@capacitor/app": "^1.1.0",
    "@capacitor/cli": "^3.4.1",
    "@capacitor/core": "^3.4.1",
    "@capacitor/device": "^1.1.2",
    "@capacitor/ios": "^3.4.1",
    "@capacitor/keyboard": "^1.2.2",
    "@capacitor/splash-screen": "^1.2.2",
    "@capacitor/status-bar": "^1.0.8",
    "@capacitor/storage": "^1.2.4",
    "@cheprasov/react-qrcode": "^1.0.1",
    "@ionic-native/clipboard": "^5.36.0",
    "@ionic-native/core": "^5.36.0",
    "@ionic-native/screen-orientation": "^5.36.0",
    "@ionic/react": "^5.8.1",
    "@ionic/react-router": "^5.8.1",
    "@types/lodash.merge": "^4.6.6",
    "antd-css-utilities": "^1.0.2",
    "axios": "^0.26.0",
    "bip32": "^2.0.6",
    "bitcoinjs-lib": "^5.2.0",
    "buffer-json": "^2.0.0",
    "capacitor-secure-storage-plugin": "^0.6.2",
    "classnames": "^2.3.1",
    "cordova-clipboard": "^1.3.0",
    "decimal.js": "^10.3.1",
    "es6-promise-plugin": "^4.2.2",
    "ionicons": "^6.0.1",
    "lodash.merge": "^4.6.2",
    "moment": "^2.29.1",
    "pegin": "^0.1.5",
    "qs": "^6.10.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.2",
    "redux-saga": "^1.1.3",
    "reselect": "^4.1.5",
    "scrypt-js": "^3.0.1",
    "slip77": "0.2.0",
    "tdex-sdk": "1.7.20",
    "tiny-secp256k1": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@ionic/cli": "^6.18.1",
    "@ionic/eslint-config": "^0.3.0",
    "@ionic/prettier-config": "^2.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@types/bs58check": "^2.1.0",
    "@types/buffer-json": "^2.0.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.21",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.22",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "cordova-res": "^0.15.4",
    "cypress": "8.4.1",
    "cypress-localstorage-commands": "^1.7.0",
    "eslint-plugin-cypress": "^2.12.1",
    "jest-canvas-mock": "^2.3.1",
    "prettier": "^2.5.1",
    "redux-devtools-extension": "^2.13.9",
    "sass": "^1.49.8",
    "tdex-react-scripts-v4": "^4.0.7",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "resolutions": {
    "blech32": "1.0.1"
  }
}
