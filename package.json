    "@ionic/react-hooks": "0.0.8",
    "@ionic/react-router": "^5.4.1",
    "axios": "^0.21.0",
    "buffer-json": "^2.0.0",
    "capacitor-secure-storage-plugin": "^0.5.0",
    "classnames": "^2.2.6",
    "com-badrit-base64": "^0.2.0",
    "cordova-clipboard": "^1.3.0",
    "cordova-plugin-screen-orientation": "^3.0.2",
    "es6-promise-plugin": "^4.2.2",
    "ionicons": "^5.0.0",
    "jetifier": "^1.6.6",
    "ldk": "^0.2.1",
    "liquidjs-lib": "^5.1.7",
    "moment": "^2.29.1",
