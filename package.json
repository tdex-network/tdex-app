{
  "name": "tdex-app",
  "version": "0.0.17-build26",
  "private": true,
  "description": "Mobile app for making trades on TDEX",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "prettier": "@ionic/prettier-config",
  "scripts": {
    "android": "ionic capacitor run android",
    "android:serve": "ionic capacitor run android -l --external",
    "build": "ionic build",
    "build:android": "ionic capacitor build android --no-open",
    "build:ios": "ionic capacitor build ios --no-open",
    "eslint": "eslint \"./src/**/*.{ts,tsx}\" \"./test/**/*.{ts,tsx}\"",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write",
    "ios": "ionic capacitor run ios",
    "ios:serve": "ionic capacitor run ios -l --external",
    "lint": "npm run eslint && npm run prettier -- --check",
    "prettier": "prettier \"./src/**/*.{ts,tsx}\" \"./test/**/*.{ts,tsx}\"",
    "resources": "cordova-res ios --skip-config --copy && cordova-res android --skip-config --copy",
    "serve": "ionic serve",
    "sync": "ionic cap sync",
    "tdex:clean": "bash scripts/tdexd-clean.sh",
    "tdex:run": "bash scripts/tdexd-run.sh",
    "test": "BABEL_ENV=test npx jest"
  },
  "dependencies": {
    "@capacitor-community/barcode-scanner": "^2.0.0",
    "@capacitor-community/react-hooks": "^0.0.11",
    "@capacitor/android": "^3.0.0",
    "@capacitor/app": "^1.0.0",
    "@capacitor/cli": "^3.0.0",
    "@capacitor/core": "^3.0.0",
    "@capacitor/device": "^1.0.0",
    "@capacitor/ios": "^3.0.0",
    "@capacitor/keyboard": "^1.0.0",
    "@capacitor/splash-screen": "^1.0.0",
    "@capacitor/status-bar": "^1.0.0",
    "@capacitor/storage": "^1.0.0",
    "@cheprasov/react-qrcode": "^1.0.1",
    "@ionic-native/clipboard": "^5.33.1",
    "@ionic-native/core": "^5.33.1",
    "@ionic-native/screen-orientation": "^5.33.1",
    "@ionic/react": "^5.6.8",
    "@ionic/react-router": "^5.6.8",
    "axios": "^0.21.1",
    "buffer-json": "^2.0.0",
    "capacitor-secure-storage-plugin": "^0.6.2",
    "classnames": "^2.3.1",
    "cordova-clipboard": "^1.3.0",
    "es6-promise-plugin": "^4.2.2",
    "ionicons": "^5.5.1",
    "liquidjs-lib": "^5.1.12",
    "moment": "^2.29.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "scrypt-js": "^3.0.1",
    "tdex-sdk": "1.6.7"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@ionic/cli": "^6.16.2",
    "@ionic/eslint-config": "^0.3.0",
    "@ionic/prettier-config": "^1.0.1",
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@types/buffer-json": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.17.2",
    "@types/react": "^16.14.8",
    "@types/react-dom": "^16.9.13",
    "@types/react-redux": "^7.1.16",
    "@types/react-router": "^5.1.14",
    "@types/react-router-dom": "^5.1.7",
    "cordova-res": "^0.15.3",
    "jest-canvas-mock": "^2.3.1",
    "prettier": "^2.3.1",
    "redux-devtools-extension": "^2.13.9",
    "sass": "^1.34.1",
    "tdex-react-scripts": "4.0.3-tdex",
    "ts-jest": "^27.0.3",
    "typescript": "3.8.3"
  }
}
